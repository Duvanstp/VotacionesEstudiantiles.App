@page
@model AdministradorModel
@{
    ViewData["Title"] = "Administrador";
 
}

<!DOCTYPE cshtml>
<cshtml lang="en">
<!-- <p>Acá podrá crear el evento de votacion siga las instrucciones</p> -->
<div class="admin-wrapper-form">
    <form id="admin-form"class="form" preventDefault="true">
    <h2>Iniciar sesión como <span> Administrador</span> </h2>
    <div class="form-group">
        <label for="user">Usuario</label>
        <input type="user" class="form-control" id="user" placeholder="Ingresar usuario">
    </div>
    <div class="form-group">
        <label for="password">Contraseña</label>
        <input type="password" class="form-control" id="password" placeholder="Contraseña" required>
    </div>
    <button id="submit-btn"type="submit" class="admin-btn">Ingresar</button>
    </form>
</div>

</cshtml>