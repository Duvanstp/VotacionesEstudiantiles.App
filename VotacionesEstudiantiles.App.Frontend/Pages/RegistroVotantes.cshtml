@page
@model VotacionesEstudiantiles.App.RegistroVotantesModel
@{
    ViewData["Title"] = "RegistroAdministrador";
}

<!DOCTYPE cshtml>
<cshtml lang="en">
 <div>
    <div class="admin-wrapper-form">
        <form id="admin-form"class="form" preventDefault="true">
        <h2>Registrar <span> votante</span> </h2>
        <div class="form-group">
            <label for="user">Código</label>
            <input type="user" class="form-control" id="user" placeholder="Ingresar usuario" required>
        </div>
        <div class="form-group">
            <label for="password">Contraseña</label>
            <input type="password" class="form-control" id="password" placeholder="Contraseña" required>
        </div>
        <button id="submit-btn" type="submit" class="admin-btn"
        >Ingresar</button>
        </form>
    </div>

</cshtml>
